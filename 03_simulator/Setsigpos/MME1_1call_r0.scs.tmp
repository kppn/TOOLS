            increase( enb_teid_u2, tmp2_enb_teid_u );
            increase( enb_teid_u2, enb_teid_u_index );
            
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV, active_ebi2,
	                MODIFY_BEARER_REQUEST_UE_SERV_POS_BCTXT_EBI, 1, 0xFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV, enb_teid_u2,
	                MODIFY_BEARER_REQUEST_UE_SERV_POS_BCTXT_ENB_TEID_U, 4, 0xFFFFFFFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV, enb_ipaddr2,
	                MODIFY_BEARER_REQUEST_UE_SERV_POS_BCTXT_ENB_IPADDR_U, 4, 0xFFFFFFFF);
