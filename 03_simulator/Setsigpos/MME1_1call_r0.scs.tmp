            increase( enb_teid_u3, tmp2_enb_teid_u );
            increase( enb_teid_u3, enb_teid_u_index );
            
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV, active_ebi3,
	                MODIFY_BEARER_REQUEST_UE_SERV_POS_BCTXT_EBI, 1, 0xFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV, enb_teid_u3,
	                MODIFY_BEARER_REQUEST_UE_SERV_POS_BCTXT_ENB_TEID_U, 4, 0xFFFFFFFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV, enb_ipaddr3,
	                MODIFY_BEARER_REQUEST_UE_SERV_POS_BCTXT_ENB_IPADDR_U, 4, 0xFFFFFFFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV__3G, active_ebi3,
	                MODIFY_BEARER_REQUEST_UE_SERV__3G_POS_BCTXT_EBI, 1, 0xFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV__3G, enb_teid_u3,
	                MODIFY_BEARER_REQUEST_UE_SERV__3G_POS_BCTXT_ENB_TEID_U, 4, 0xFFFFFFFF);
            setsigdata( MODIFY_BEARER_REQUEST_UE_SERV__3G, enb_ipaddr3,
	                MODIFY_BEARER_REQUEST_UE_SERV__3G_POS_BCTXT_ENB_IPADDR_U, 4, 0xFFFFFFFF);

            setvariable( enb_ipaddr3_ebi_flg, 2 );
            
	    if( mode == MODE_LTE ){
                send(MODIFY_BEARER_REQUEST_UE_SERV);
                snap("send:MODIFY_BEARER_REQUEST_UE_SERV");
	    } else {    # MODE_3G
                send(MODIFY_BEARER_REQUEST_UE_SERV__3G);
                snap("send:MODIFY_BEARER_REQUEST_UE_SERV__3G");
	    }

            increase(enb_teid_u_index);
        }

        ### Target側Nodeの場合M.B.Rsp検知不可のため
	if( node_mode == node_MME2 ){
	    transit( STATE_TEST );
	}
	if( node_mode == node_SGSN2 ){
	    transit( STATE_TEST );
	}
    }
    
    
    
    case receive(GTPv2:MODIFY_BEARER_RESPONSE) {
        snap("recv:MODIFY_BEARER_RESPONSE");
        getsigdata( res_cause, 16, 1, 0xFF );
        if( res_cause == 0x10 ) {                         # Response accepted
            snap( "Response Cause OK" );

        }
        else {                                            # Response error
            snap( "Response Cause = ", res_cause );
        }
        transit(STATE_TEST);
    }
}





