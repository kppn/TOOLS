            setsigdata( MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME,  active_ebi2,
                        MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME_POS_EBI,  1,  0xFF );

	    if( ho_kind == KIND_TAU ){
                send( MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME );
                snap( "send:MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME" );
	    }
	    if( ho_kind == KIND_TAU_IRAT ){
                send( MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_IRAT );
                snap( "send:MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_IRAT" );
	    }
	    if( ho_kind == KIND_HO ){
                send( MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME );
                snap( "send:MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME" );
            }
	    if( ho_kind == KIND_HO_INDIRECT ){
                send( MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME );
                snap( "send:MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME" );
            }

            increase( sig_send_count );
        }
        if( active_ebi3 != 0 ){
            snap( "active_ebi3 = ", active_ebi3 );

            setsigdata( MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME,  active_ebi3,
                        MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_POS_EBI,  1,  0xFF );
            setsigdata( MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_IRAT,  active_ebi3,
                        MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_IRAT_POS_EBI,  1,  0xFF );
            setsigdata( MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME,  active_ebi3,
                        MODIFY_BEARER_REQUEST_HO_WO_SGW_TARGET_MME_POS_EBI,  1,  0xFF );

	    if( ho_kind == KIND_TAU ){
                send( MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME );
                snap( "send:MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME" );
	    }
	    if( ho_kind == KIND_TAU_IRAT ){
                send( MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_IRAT );
                snap( "send:MODIFY_BEARER_REQUEST_TAU_WO_SGW_TARGET_MME_IRAT" );
